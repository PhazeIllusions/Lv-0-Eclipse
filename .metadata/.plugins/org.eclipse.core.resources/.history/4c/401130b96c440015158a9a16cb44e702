import org.teachingextensions.logo.Tortoise;


public class TurtleTesting {
public static void main(String[] args) {
	for (int i = 0; i < 100000; i++) {
		Tortoise.setSpeed(10);
		Tortoise.move(61);
		Tortoise.turn(49);
		Tortoise.move(13);
		Tortoise.turn(29);
		Tortoise.move(73);
		Tortoise.turn(78);
		Tortoise.move(59);
		Tortoise.turn(45);
		Tortoise.move(69);
	}
}
}

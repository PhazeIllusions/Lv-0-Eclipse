import java.util.Random;

import javax.swing.JOptionPane;

import org.teachingextensions.logo.Tortoise;


public class Lv_0_Eclipse {
public static void main(String[] args) {
	Tortoise.setSpeed(10);
for(int i=0; i<10000;i++){
	Tortoise.move(85);
	Tortoise.turn(78);
	Tortoise.move(51);
	Tortoise.turn(87);
	Tortoise.move(29);
	
}

}
}     






